{
  "name": "guwenbao",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "api": "node ./build/api.js",
    "dev:local": "npm run api local && NODE_ENV=dev webpack-dev-server --config build/webpack.dev.js --hot",
    "dev:both": "npm run api && NODE_ENV=dev webpack-dev-server --config build/webpack.dev.js --hot --deploy",
    "start": "npm run dev:local",
    "deploy:test": "npm run api test && webpack --config build/webpack.release.js --watch --deploy=test",
    "deploy:online": "npm run api && NODE_ENV=production webpack --config build/webpack.release.js --watch -p --deploy=online",
    "release": "npm run deploy:online",
    "test": "mocha --opts ./mocha.opts ./src/**/__tests__/*.js",
    "test-cov": "./node_modules/.bin/istanbul cover _mocha -- ./src/**/__tests__/*.js --opts mocha.opts"
  },
  "author": "xiaoyann",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ftp": "^0.3.10",
    "glob": "^7.0.3",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.19.0",
    "image-webpack-loader": "^1.8.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.4.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-tools": "^0.13.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "ajax": "xiaoyann/ajax",
    "fastclick": "^1.0.6",
    "foundation": "xiaoyann/foundation",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
